    entrypoint:
      - /bin/bash
      - -c
      - /opt/mssql/bin/sqlservr & sleep 30s && /opt/mssql-tools/bin/sqlcmd -S localhost -U sa -P P@ssw0rd1 -i /usr/src/app/init.sql && tail -f /dev/null
